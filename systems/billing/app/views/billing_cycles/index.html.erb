<p id="notice"><%= notice %></p>

<h1>Billing Cycles</h1>

<table>
  <thead>
    <tr>
      <th>Public ID</th>
      <th>Date</th>
      <th>Company Profit Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billing_cycles.each do |billing_cycle| %>
      <tr>
        <td><%= billing_cycle.public_id %></td>
        <td><%= billing_cycle.date %></td>
        <td><%= billing_cycle.company_profit_amount %> $</td>
      </tr>
    <% end %>
  </tbody>
</table>
